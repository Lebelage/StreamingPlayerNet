<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--#region COLORS -->
    <!--#region Standard colors-->
    <Color x:Key="color_White" A="255" R="255" G="255" B="255"/>
    <Color x:Key="color_Red" A="255" R="255" G="0" B="0"/>
    <Color x:Key="color_DarkRed" A="255" R="127" G="0" B="0"/>
    <Color x:Key="color_Black" A="255" R="0" G="0" B="0"/>
    <!--#endregion-->

    <Color x:Key="color_MainBackround" A="255" R="31" G="31" B="31"/>
    <Color x:Key="color_MainDarkBackground" A="255" R="15" G="2" B="67"/>
    <Color x:Key="color_MainDisabledBackground" A="255" R="75" G="62" B="127"/>
    <Color x:Key="color_MainBorder" A="255" R="113" G="96" B="232"/>
    <Color x:Key="color_MainForeground" A="255" R="220" G="220" B="220"/>
    <Color x:Key="color_MainDisabledForeground" A="255" R="170" G="170" B="170"/>

    <Color x:Key="color_StatusBarBackground" A="255" R="138" G="138" B="138"/>

    <Color x:Key="color_ButtonDefaultBackground" A="255" R="86" G="67" B="161"/>
    <Color x:Key="color_ButtonDefaultBorder" A="255" R="112" G="112" B="112"/>
    <Color x:Key="color_ButtonMouseOverBackground" A="255" R="61" G="61" B="61"/>
    <Color x:Key="color_ButtonMouseOverBorder" A="255" R="163" G="163" B="163"/>
    <Color x:Key="color_ButtonPressedBackground" A="255" R="46" G="46" B="46"/>
    <Color x:Key="color_ButtonPressedBorder" A="255" R="138" G="138" B="138"/>
    <Color x:Key="color_ButtonDisabledBackground" A="255" R="75" G="75" B="107"/>
    <!--#endregion-->

    <!--#region BRUSHES -->
    <!--#region Standard brushes-->
    <SolidColorBrush x:Key="brush_White" Color="{StaticResource color_White}"/>
    <SolidColorBrush x:Key="brush_Red" Color="{StaticResource color_Red}"/>
    <SolidColorBrush x:Key="brush_DarkRed" Color="{StaticResource color_DarkRed}"/>
    <SolidColorBrush x:Key="brush_Black" Color="{StaticResource color_Black}"/>
    <!--#endregion-->

    <SolidColorBrush x:Key="brush_MainBackground" Color="{StaticResource color_MainBackround}"/>
    <SolidColorBrush x:Key="brush_MainDarkBackground" Color="{StaticResource color_MainDarkBackground}"/>
    <SolidColorBrush x:Key="brush_MainDisabledBackground" Color="{StaticResource color_MainDisabledBackground}"/>
    <SolidColorBrush x:Key="brush_MainBorder" Color="{StaticResource color_MainBorder}"/>
    <SolidColorBrush x:Key="brush_MainForeground" Color="{StaticResource color_MainForeground}"/>
    <SolidColorBrush x:Key="brush_MainDisabledForeground" Color="{StaticResource color_MainDisabledForeground}"/>

    <SolidColorBrush x:Key="brush_StatusBarBackground" Color="{StaticResource color_StatusBarBackground}"/>

    <SolidColorBrush x:Key="brush_ButtonDefaultBackground" Color="{StaticResource color_ButtonDefaultBackground}"/>
    <SolidColorBrush x:Key="brush_ButtonDefaultBorder" Color="{StaticResource color_ButtonDefaultBorder}"/>
    <SolidColorBrush x:Key="brush_ButtonMouseOverBackground" Color="{StaticResource color_ButtonMouseOverBackground}"/>
    <SolidColorBrush x:Key="brush_ButtonMouseOverBorder" Color="{StaticResource color_ButtonMouseOverBorder}"/>
    <SolidColorBrush x:Key="brush_ButtonPressedBackground" Color="{StaticResource color_ButtonPressedBackground}"/>
    <SolidColorBrush x:Key="brush_ButtonPressedBorder" Color="{StaticResource color_ButtonPressedBorder}"/>
    <SolidColorBrush x:Key="brush_ButtonDisabledBackground" Color="{StaticResource color_ButtonDisabledBackground}"/>
    <SolidColorBrush x:Key="brush_ButtonDisabledBorder" Color="{StaticResource color_ButtonDefaultBorder}"/>
    
    <SolidColorBrush x:Key="brush_TextBoxDefaultBackground" Color="{StaticResource color_White}"/>
    <SolidColorBrush x:Key="brush_TextBoxDefaultForeground" Color="{StaticResource color_Black}"/>
    <SolidColorBrush x:Key="brush_TextBoxDefaultBorder" Color="{StaticResource color_Black}"/>
    <!--#endregion-->

</ResourceDictionary>